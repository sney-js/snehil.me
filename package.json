{
  "name": "react-boilerplate-detailed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:lint": "eslint ./src --ext .ts --ext .tsx",
    "lint:prettier": "prettier --write \"./src/**/*.{js,ts,tsx,json,scss}\"",
    "eject": "react-scripts eject",
    "svgr": "svgr --config-file svgrrc.config.js --ext tsx -d src/elements/SvgElements public/assets/svg-elements",
    "storybook": "start-storybook -s ./public -p 9001 -s ./public/assets",
    "contentful:types": "contentful-typescript-codegen --output src/contentful/@types/contentful.d.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
