    "@storybook/theming": "6.5.14",
    "@svgr/cli": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-async-helpers": "^0.9.2",
